  "weightLossRecipes": [
    {
      "id": 1,
      "name": "Grilled Chicken with Asparagus",
      "ingredients": [
        "2 chicken breasts",
        "1 bunch of asparagus",
        "1 tbsp olive oil",
        "1 tsp garlic powder",
        "1 tsp onion powder",
        "salt and pepper"
      ],
      "directions": [
        "Preheat grill to medium-high heat.",
        "Season chicken breasts with garlic powder, onion powder, salt and pepper.",
        "Drizzle asparagus with olive oil and season with salt and pepper.",
        "Grill chicken and asparagus for 8-10 minutes on each side, until chicken is cooked through and asparagus is tender."
      ]
    },
    {
      "id": 2,
      "name": "Broiled Salmon with Broccoli",
      "ingredients": [
        "2 salmon fillets",
        "1 head of broccoli",
        "1 tbsp olive oil",
        "1 tsp lemon juice",
        "1 tsp dried dill",
        "salt and pepper"
      ],
      "directions": [
        "Preheat broiler on high.",
        "Season salmon fillets with lemon juice, dill, salt and pepper.",
        "Toss broccoli with olive oil, salt and pepper.",
        "Place salmon fillets and broccoli on a baking sheet and broil for 10-12 minutes, until salmon is cooked through and broccoli is tender."
      ]
    },
    {
      "id": 3,
      "name": "Turkey Chili",
      "ingredients": [
        "1 lb ground turkey",
        "1 can of black beans",
        "1 can of kidney beans",
        "1 can of diced tomatoes",
        "1 onion, chopped",
        "2 cloves of garlic, minced",
        "1 tbsp chili powder",
        "1 tsp cumin",
        "1 tsp paprika",
        "salt and pepper"
      ],
      "directions": [
        "In a large pot, brown ground turkey with onion and garlic.",
        "Add chili powder, cumin, paprika, salt and pepper and cook for 2 minutes.",
        "Add black beans, kidney beans and diced tomatoes with their juice.",
        "Simmer for 15-20 minutes, until chili is thick and flavorful."
      ]
    },
    {
      "id": 4,
      "name": "Greek Yogurt with Berries and Almonds",
      "ingredients": [
        "1 cup of Greek yogurt",
        "1 cup of mixed berries",
        "1/4 cup of almonds, chopped",
        "1 tbsp honey (optional)"
      ],
      "directions": [
        "Spoon Greek yogurt into a bowl.",
        "Top with mixed berries and chopped almonds.",
        "Drizzle with honey if desired."
      ]
    },
    {
      "id": 5,
      "name": "Quinoa Salad with Cucumbers and Tomatoes",
      "ingredients": [
        "1 cup of cooked quinoa",
        "1 cucumber, diced",
        "1 cup of cherry tomatoes, halved",
        "1/4 cup of feta cheese",
        "2 tbsp of olive oil",
        "1 tbsp of lemon juice",
        "salt and pepper"
      ],
      "directions": [
        "In a large bowl, combine cooked quinoa, diced cucumber and halved cherry tomatoes.",
        "Add crumbled feta cheese and toss to combine.",
        "Whisk together olive oil, lemon juice, salt and pepper and pour over salad.",
        "Toss to coat and serve chilled."
      ]
    }
  ],
