  "buildMuscleRecipes": [
    {
      "id": 1,
      "name": "Grilled Chicken and Vegetables",
      "ingredients": [
        "2 boneless, skinless chicken breasts",
        "1 zucchini, sliced",
        "1 bell pepper, sliced",
        "1 red onion, sliced",
        "2 tbsp olive oil",
        "2 cloves garlic, minced",
        "1/2 tsp dried thyme",
        "salt and pepper"
      ],
      "directions": [
        "Preheat grill to medium-high heat.",
        "In a large bowl, whisk together olive oil, garlic, thyme, salt and pepper.",
        "Add chicken and vegetables to the bowl and toss to coat in the marinade.",
        "Grill chicken and vegetables for 6-8 minutes per side, until chicken is cooked through and vegetables are tender.",
        "Serve hot."
      ]
    },
    {
      "id": 2,
      "name": "Beef and Broccoli Stir-Fry",
      "ingredients": [
        "1 lb beef sirloin, sliced",
        "2 cups broccoli florets",
        "1 bell pepper, sliced",
        "1 onion, sliced",
        "2 garlic cloves, minced",
        "1/4 cup soy sauce",
        "1/4 cup beef broth",
        "1 tbsp cornstarch",
        "1 tbsp vegetable oil",
        "2 cups cooked rice"
      ],
      "directions": [
        "In a small bowl, whisk together soy sauce, beef broth and cornstarch.",
        "Heat vegetable oil in a large skillet over high heat.",
        "Add beef sirloin and cook for 2-3 minutes on each side, until browned.",
        "Add broccoli, bell pepper, onion and garlic and stir-fry for 2-3 minutes.",
        "Pour soy sauce mixture over the beef and vegetables and stir-fry for another 1-2 minutes, until sauce is thickened.",
        "Serve over cooked rice."
      ]
    },
    {
      "id": 3,
      "name": "Salmon and Sweet Potato Hash",
      "ingredients": [
        "2 salmon fillets",
        "2 sweet potatoes, peeled and diced",
        "1 red bell pepper, diced",
        "1 onion, diced",
        "2 cloves garlic, minced",
        "2 tbsp olive oil",
        "1 tsp smoked paprika",
        "salt and pepper"
      ],
      "directions": [
        "Preheat oven to 400°F.",
        "In a large bowl, toss sweet potatoes, red bell pepper, onion, garlic, olive oil, smoked paprika, salt and pepper together.",
        "Spread the mixture onto a baking sheet and bake for 25-30 minutes, until sweet potatoes are tender.",
        "Season salmon fillets with salt and pepper and bake in the oven for 10-12 minutes, until cooked through.",
        "Serve salmon fillets on top of the sweet potato hash."
      ]
    },
    {
      "id": 4,
      "name": "Quinoa and Black Bean Bowl",
      "ingredients": [
        "1 cup quinoa",
        "1 can black beans, drained and rinsed",
        "1 avocado, sliced",
        "1 bell pepper, diced",
        "1/4 cup cilantro, chopped",
        "1/4 cup lime juice",
        "2 tbsp olive oil",
        "salt and pepper"
      ],
      "directions": [
        "Cook quinoa according to package instructions.",
        "In a large bowl, combine cooked quinoa, black beans, avocado, bell pepper, cilantro, lime juice, olive oil, salt and pepper.",
