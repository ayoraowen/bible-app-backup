  "weightGainRecipes": [
    {
      "id": 1,
      "name": "Peanut Butter Banana Smoothie",
      "ingredients": [
        "2 ripe bananas",
        "2 tbsp peanut butter",
        "1 cup whole milk",
        "1 cup ice"
      ],
      "directions": [
        "Combine all ingredients in a blender and blend until smooth.",
        "Pour into a glass and serve immediately."
      ]
    },
    {
      "id": 2,
      "name": "Loaded Sweet Potato",
      "ingredients": [
        "1 large sweet potato",
        "1/2 cup black beans",
        "1/2 cup shredded cheddar cheese",
        "1/4 cup salsa",
        "1 tbsp olive oil",
        "salt and pepper"
      ],
      "directions": [
        "Preheat oven to 400°F.",
        "Pierce sweet potato with a fork a few times and rub with olive oil.",
        "Bake for 45-50 minutes, until tender.",
        "Top with black beans, cheddar cheese and salsa.",
        "Sprinkle with salt and pepper and serve hot."
      ]
    },
    {
      "id": 3,
      "name": "Beef Stir-Fry",
      "ingredients": [
        "1 lb beef sirloin, sliced",
        "1 bell pepper, sliced",
        "1 onion, sliced",
        "2 garlic cloves, minced",
        "1/4 cup soy sauce",
        "1/4 cup brown sugar",
        "2 tbsp vegetable oil",
        "2 cups cooked rice"
      ],
      "directions": [
        "In a small bowl, whisk together soy sauce and brown sugar.",
        "Heat vegetable oil in a large skillet over high heat.",
        "Add beef sirloin and cook for 2-3 minutes on each side, until browned.",
        "Add bell pepper, onion and garlic and stir-fry for 2-3 minutes.",
        "Pour soy sauce mixture over the beef and vegetables and stir-fry for another 1-2 minutes.",
        "Serve over cooked rice."
      ]
    },
    {
      "id": 4,
      "name": "Avocado and Egg Toast",
      "ingredients": [
        "2 slices of whole wheat bread",
        "1 ripe avocado, mashed",
        "2 eggs",
        "1 tbsp butter",
        "salt and pepper"
      ],
      "directions": [
        "Toast the bread until golden brown.",
        "Spread mashed avocado on each slice of toast.",
        "Melt butter in a skillet over medium heat.",
        "Crack eggs into the skillet and fry for 2-3 minutes, until cooked to your liking.",
        "Season eggs with salt and pepper and place one egg on top of each slice of toast.",
        "Serve immediately."
      ]
    },
    {
      "id": 5,
      "name": "Pasta with Meat Sauce",
      "ingredients": [
        "1 lb ground beef",
        "1 onion, chopped",
        "2 garlic cloves, minced",
        "1 can of tomato sauce",
        "1 can of diced tomatoes",
        "1 tsp dried oregano",
        "1/2 tsp dried basil",
        "1/4 tsp red pepper flakes",
        "salt and pepper",
        "1 lb spaghetti"
      ],
      "directions": [
        "In a large pot, brown ground beef with onion and garlic.",
        "Add tomato sauce, diced tomatoes, oregano, basil, red pepper flakes, salt and pepper.",
        "Simmer for 20-30 minutes, until sauce is thick and flavorful.",
        "Cook spaghetti according to package instructions and drain.",
        "Toss spaghetti with meat sauce and serve hot."
      ]
    }
